{
  "manifest_version": 3,
  "name": "书签管理器",
  "description": "书签管理器",
  "version": "1.0",
  "permissions": ["bookmarks", "storage","favicon"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["_favicon/*"],
      "matches": ["<all_urls>"],
      "extension_ids": ["*"]
    }
  ]
}
